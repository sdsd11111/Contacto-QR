generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model registraya_vcard_registros {
  id                  String    @id @default(uuid()) @db.VarChar(36)
  slug                String?   @unique @db.VarChar(255)
  created_at          DateTime? @db.DateTime(0)
  nombre              String?   @db.VarChar(255)
  whatsapp            String?   @db.VarChar(50)
  email               String?   @unique @db.VarChar(255)
  profesion           String?   @db.VarChar(255)
  empresa             String?   @db.VarChar(255)
  bio                 String?   @db.Text
  etiquetas           String?   @db.Text
  plan                String?   @db.VarChar(50)
  foto_url            String?   @db.Text
  comprobante_url     String?   @db.Text
  status              String?   @db.VarChar(50)
  delivery_deadline   DateTime? @db.DateTime(0)
  direccion           String?   @db.Text
  web                 String?   @db.Text
  instagram           String?   @db.Text
  linkedin            String?   @db.Text
  galeria_urls        Json?
  facebook            String?   @db.Text
  tiktok              String?   @db.Text
  productos_servicios String?   @db.Text
  google_business     String?   @db.Text
  seller_id           Int?
  commission_status   String?   @default("pending") @db.VarChar(50)
}
